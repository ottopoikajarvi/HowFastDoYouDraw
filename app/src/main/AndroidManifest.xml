<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mobComp2020.howfastdoyoudraw">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.mobComp2020.howfastdoyoudraw.SettingsActivity"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_settings"
            android:parentActivityName="com.mobComp2020.howfastdoyoudraw.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.mobComp2020.howfastdoyoudraw.MainActivity" />
        </activity>
        <activity android:name="com.mobComp2020.howfastdoyoudraw.PlayActivity"
            android:screenOrientation="portrait"/>
        <activity android:name="com.mobComp2020.howfastdoyoudraw.HighScoreActivity" android:screenOrientation="portrait"/>
        <activity android:name="com.mobComp2020.howfastdoyoudraw.MainActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>